
{
  "compilerOptions": {
    "target": "ES6", 
    "module": "commonjs", 
    "strict": true,
    "esModuleInterop": true, 
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist", 
    "resolveJsonModule": true,
    "rootDir": "./src"
  },
  "include": [ "src/**/*", "./**/*"],
  "exclude": ["node_modules"]
}


